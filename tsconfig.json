{
    "compilerOptions": {
        "outDir": "./dist",
        "module": "ESNext",
        "target": "es5",
        "lib": ["dom", "esnext"],
        "jsx": "react-jsx",
        "declaration": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": ["src/**/*"]
}
  